{
  "name": "music-notation-app",
  "productName": "Music Notation Studio",
  "private": true,
  "version": "1.0.0",
  "description": "A music notation application with keyboard input, MIDI playback, and dual notation systems",
  "author": "Music Notation Studio",
  "main": "dist-electron/main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && electron-builder",
    "preview": "vite preview",
    "electron:dev": "vite",
    "electron:build": "vite build && electron-builder"
  },
  "devDependencies": {
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^9.2.1",
    "electron": "^40.1.0",
    "electron-builder": "^26.4.0",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "vite-plugin-electron": "^0.29.0",
    "vite-plugin-electron-renderer": "^0.14.6"
  },
  "dependencies": {
    "@tonejs/midi": "^2.0.28",
    "html2canvas": "^1.4.1",
    "i18next": "^25.8.0",
    "jianpurender": "^1.2.0",
    "jspdf": "^4.0.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-i18next": "^16.5.4",
    "tone": "^15.1.22",
    "vexflow": "^5.0.0"
  },
  "build": {
    "appId": "com.musicnotation.studio",
    "productName": "Music Notation Studio",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*"
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "category": "public.app-category.music",
      "icon": "build/icon.icns"
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    }
  }
}
